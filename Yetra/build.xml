<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project Yetra">
	<!--this file was created by Eclipse Runnable JAR Export Wizard-->
	<!--ANT 1.7 is required                                        -->
	<target name="create_run_jar">
		<delete dir="dist" />
		<jar destfile="dist/yetra.jar" filesetmanifest="mergewithoutmain">
			<manifest>
				<attribute name="Main-Class" value="com.yetra.TestMain" />
				<attribute name="Class-Path" value="." />
			</manifest>
			<fileset dir="bin" />
		</jar>
	</target>
</project>